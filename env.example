# Copy this file to .env.local and fill in your actual values
# This file is safe to commit to GitHub as it only contains placeholder values

# Mapbox API Token (get from https://account.mapbox.com/access-tokens/)
NEXT_PUBLIC_MAPBOX_TOKEN=REPLACE_ME

# Supabase Configuration (get from your Supabase project settings)
NEXT_PUBLIC_SUPABASE_URL=REPLACE_ME
NEXT_PUBLIC_SUPABASE_ANON_KEY=REPLACE_ME

# Supabase Service Role Key (for server-side operations like seeding)
SUPABASE_SERVICE_ROLE_KEY=REPLACE_ME
